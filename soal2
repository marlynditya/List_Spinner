def cclist(x):
    list_diputar = []
    list_len = len(x)
    for i in range(list_len):
        list_diputar.append([])
    for i in range(list_len):
        for j in range(list_len):
            list_diputar[i].append(x[j].pop(-1))
    return list_diputar

list_awal = [[1, 2, 3],
            [4, 5, 6],
            [7, 8, 9]]

print(cclist(list_awal))
